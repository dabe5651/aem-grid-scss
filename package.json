{
  "name": "aem-grid-scss",
  "version": "1.0.0",
  "description": "Unofficial SCSS port of the AEM responsive grid",
  "main": "aem-grid.scss",
  "scripts": {
    "build:scss": "node-sass src/scss/aem-grid.scss  dist/scss-grid.css --precision 8",
    "build:scss:watch": "node-sass --watch src/scss/aem-grid.scss  dist/scss-grid.css --precision 8",
    "build:less": "lessc src/less/grid.less >  dist/less-grid-original.css",
    "build": "npm run build:scss && npm run build:less"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jonjhiggins/aem-grid-scss.git"
  },
  "keywords": [
    "AEM",
    "grid"
  ],
  "author": "Jon Higgins <jon@jonhiggins.co.uk>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jonjhiggins/aem-grid-scss/issues"
  },
  "homepage": "https://github.com/jonjhiggins/aem-grid-scss#readme",
  "devDependencies": {
    "less": "^3.7.1",
    "node-sass": "^4.9.2"
  }
}
